    <header class="absolute main">
        <nav class="relative full">
            <a href="#">
                <h1 class="main absolute block">
                    <%= image_tag('logo.png', alt: 'walawala logo') %>
                  <!-- <img alt="walawala logo" src="images/logo.png"/> -->
                </h1>
            </a>
            <div class="main_player absolute">
              <div class="playing_btn absolute cursor_pointer">
                    <div class="start_btn absolute full">
                        <!-- <img class="poz_center" src="images/start_icon.png"> -->
                        <%= image_tag('start_icon.png', class: 'poz_center') %>
                    </div>
                    <div class="pause_btn absolute full">
                        <%= image_tag('pause_icon.png', class: 'poz_center') %>
                        <!-- <img class="poz_center" src="images/pause_icon.png"> -->
                    </div>
                </div>
              <div class="time_bar absolute">
                    <div class="time_line absolute"></div>
                    <div class="player_info absolute" id="player_info">
                        <div class="player_info_name inline_block">節目名稱 - 集名稱</div>
                        <div class="player_info_time absolute inline_block">00:00</div>
                    </div>
                    <div class="player_info_filter absolute full" id="player_info_filter"></div>
                    <div class="time_line_bar absolute" id="time_line_bar"></div>
                </div>
                <div class="next_play_btn absolute"></div>
              <div class="volume_sec absolute">
                    <div class="volume_btn absolute full cursor_pointer"></div>
                    <div class="volume_bar absolute">
                        <div class="volume_bar_limit absolute cursor_pointer">
                            <div class="volume_color absolute"></div>
                            <div class="volume_drag_body absolute">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <ul class="inline_block float_right">
                <li class="inline_block cursor_pointer main_search">
                    <span class="main_search_btn">
                        <%= image_tag('icon_search.png') %>
                        <!-- <img src="images/icon_search.png"> -->
                    </span>

<!--                         <input class="relative search_input" type="text" placeholder="輸入">
                    </span>
                     <span class="search_word"></span>
                </li>-->
                <li class="inline_block">
                    <!-- <a href="#category"> -->
                    <%= link_to(path_with_hash_tag(categories_path)) do %>
                        <span>
                            <%= image_tag('icon_category.png') %>
                            <!-- <img src="images/icon_category.png"> -->
                        </span>
                    <% end %>
                    <!-- </a> -->
                </li>


                <% if user_signed_in? %>
                <li class="inline_block">
                    <!-- <a href="#upload"> -->
                    <%= link_to(path_with_hash_tag(programs_path)) do %>
                        <span>
                            <%= image_tag('icon_upload.png') %>
                            <!-- <img src="images/icon_upload.png"> -->
                        </span>
                    <% end %>
                    <!-- </a> -->
                </li>

               
                  <li class="inline_block relative user_account user_account_in hide">
                  
                    <!-- <li class="inline_block relative user_account hide"> -->
                      <div class="absolute user_account_bg"></div>
                      <div class="relative user_account_in">
                          <span>
                              <%= image_tag('icon_userdemo.png') %>
                              <!-- <img src="images/icon_userdemo.png"> -->
                          </span>
                      </div>
                  </li>
                  <div class="absolute triangle"></div>
                  <div class="absolute login_menu">
                    <%= link_to(path_with_hash_tag(edit_user_registration_path)) do %>
                      <div class="absolute login_data_btn">個人資料修改</div>
                    <% end %>
                      <!-- <a href="#setting">
                          <div class="absolute login_data_btn">個人資料修改</div>
                      </a> -->
                    <div class="absolute logout_btn">
                      <%= link_to('登出', destroy_user_session_path, method: :delete, remote: true) %>
                    </div>
                  </div>
                <% end %>

                

                <% if user_signed_in? %>
                  <li class="inline_block relative user_account user_account_in"> 
                    <!-- <div class="absolute user_account_bg"></div>      -->
                    <span>
                      <%= image_tag('icon_login.png') %>
                    </span>   
                  </li>
                <% else %>

                  <li class="inline_block relative user_account_not_login">
                      <%= link_to(path_with_hash_tag(new_user_session_path)) do %>  
                          <span>
                            <%= image_tag('icon_login.png') %>
                          </span>
                      <% end %>
                  </li>
                <% end %>


                <!-- <li class="inline_block relative user_account user_account_in hide">
                    <div class="absolute user_account_bg"></div>
                    <div class="relative user_account_in">
                        <span>
                            <%#= image_tag('icon_userdemo.png') %>
                            
                        </span>帳號
                    </div>
                </li>
                <div class="absolute login_menu">
                    <a href="#setting">
                        <div class="absolute login_data_btn">個人資料修改</div>
                    </a>
                    <div class="absolute logout_btn">登出</div>
                </div>
                <li class="inline_block relative user_account_not_login">
                    <a href='<%#= path_with_hash_tag(new_user_session_path) %>'> 
                        <span>
                            <%#= image_tag('icon_login.png') %>
                            
                        </span>
                    </a>
                </li> -->




            </ul>
        </nav>
        <audio class="player_audio" id="player_audio"></audio>
    </header>