
// console.log($('div.program_<%= @program.id %>').get(0));

$('.program_<%= @program.id %>').each(function() {

  if($(this).parentsUntil($.Body,'.container').hasClass('s_c_i_followed')){
    $(this).parentsUntil($.Body,'.container').removeClass('s_c_i_followed');
  } else {
    $(this).parentsUntil($.Body,'.container').addClass('s_c_i_followed');
  }

  $(this).closest('.subscribe_program_<%= @program.id %>').attr("href", "<%= cancel_subscription_program_path(@program)%>");

});


