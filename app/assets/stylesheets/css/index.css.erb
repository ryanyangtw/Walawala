.playAudio,.play { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -36px -35px; width: 32px; height: 32px; margin-left:-21px;margin-top:5px; }
.playAudio:hover { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -36px -3px; }
.pauseAudio { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -2px -35px; width: 32px; height: 32px; margin-left:-21px;margin-top:5px; }
.pauseAudio:hover { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -2px -3px; }
/*9/23 modify-end*/
.shuffle { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -28px -96px; width: 32px; height: 32px; margin:auto; }
