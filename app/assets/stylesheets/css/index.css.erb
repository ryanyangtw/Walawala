.left { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -60px -99px; width: 24px; height: 24px; margin-left:286px; margin-top:-24px; opacity:0.4; }
.left:not(.disabled):hover { opacity:1; }
.left.disabled, .right.disabled { opacity:0.4; cursor:default; }
.right { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -88px -75px; width: 25px; height: 24px; margin-left:286px; margin-top:-24px;  opacity:0.4; }
.right:not(.disabled):hover { opacity:1; }
.repeat { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -4px -103px; width: 21px; height: 21px; margin:auto; margin-top:7px; }
.repeat:hover, .repeat.loopActive { background: url(<%= asset_path('audio/audio_icons.png') %>) no-repeat -4px -79px; }
