.mute { background-position: -34px -127px; }
.progress { clear:both; height:42px; background-color:rgba(102, 102, 102, 0.4); width:230px; cursor:pointer; position:absolute;margin-top:-33px;margin-left:47px;border: 1px solid white;opacity: 0.3; }
.p_progress { clear:both; height:5px; background-color:#666666; width:520px; cursor:pointer; position:absolute;opacity:0.2;margin-top:25px;margin-left:70px; }
.progress .updateProgress { width:0px; background-color:#00BD9B; height:100%; float:left; position:relative; }
.p_progress .updateProgress { width:0px; background-color:#00BD9B; height:100%; float:left; position:relative; }
.volumeControl { position: relative; margin: 4px 0px; display:none;opacity:.7;}
.volumeControl .updateProgress { display:inline-block; vertical-align:middle; margin-top:2px; }
input[type="range"] { -webkit-appearance: none; -moz-appearance: none; background-color: #00BD9B; height: 2px; margin-left: 284px;margin-top: 42px;border: 5px solid white;width: 90px;transform: rotate(270deg);}
.audioDetails { clear:both; color:#FFF; font-size:0.625rem; padding:5px;margin-left:46px;margin-top:-30px; width:220px;}
.p_audioDetails { clear:both;font-size:0.625rem; padding:5px;margin-left:46px;margin-top:0px; width:220px;}
.audioTime { display: inline-block; float: right;  }
.songPlay { display:inline-block; }
.seekableTrack { width: 0%; background-color:rgba(102, 102, 102, 0.4); height: 4px; position: absolute; z-index: 0; display: block; }
.seekableTrack, .progress .updateProgress { -webkit-transition: width 0.6s ease; -moz-transition: width 0.6s ease; -o-transition: width 0.6s ease; transition: width 0.6s ease; }


.hide {display:none;}
.add_play_list {width:100%;height:100px;overflow:hidden;}

/*modify0810*/
/* select_class */
.select_class     {padding: 0px 0px; min-height: 98%;width:980px;margin:0px auto 0px auto;}
.s_c_title        {position:relative;font-size:1.625rem;color:#F2F2F2;width:750px;height:40px;padding:30px 30px 0px 40px;display: inline-block}
.s_c_submit       {position:relative; display: inline-block;width:100px;height:30px;font-size:1rem;border:none;background-color:#3B313B ;border-radius:10px;color:#FFF;transition:background-color .3s}
.s_c_submit:hover   {position:relative; display: inline-block;width:100px;height:30px;font-size:1rem;border:none;background-color:#453B45 ;border-radius:10px;color:#FFF;}
.s_c_hr         {position:relative;width:900px;margin: 5px auto 20px auto;height:1px;border:none;background-color: #999; }
.s_c_content      {position:relative;width:900px;margin: 0px auto 125px auto;}
.s_c_item       {position:relative;width:140px;height:140px;display: inline-block;margin:15px 5px 0px 0px;}
.s_c_black        {position:absolute;background:rgba(0,0,0,0.7);width:140px;height:140px;display: inline-block;margin:0px 5px 0px 0px;z-index:10;transition: background .3s;}
.s_c_black:hover      {position:absolute;background:rgba(0,0,0,0.3);width:140px;height:140px;display: inline-block;margin:0px 5px 0px 0px;z-index:10;}
.s_c_black:hover .s_c_subtitle {transform: scale(1.25,1.25);}
.s_c_black:hover .s_c_icon     {transform: scale(1.05,1.05);}
.s_c_pic        {position:absolute;display: inline-block;background-image: url(<%= asset_path('s_c_pic1.png') %>); background-repeat: no-repeat;width:100%;height:100%;background-size:contain;}
.s_c_checkbox     {position:absolute;width:10px;height:10px;display:none;z-index:100;}
.s_c_box        {position:absolute;z-index:20;width:60px;height:60px;margin:20px 40px 0px auto;left:40px;}
.s_c_icon       {position:absolute;width:100%;z-index:20;transition: all 0.3s;}
.s_c_subtitle           {position:absolute;width:100%;z-index:20;font-size:1rem;color:#F2F2F2;top:100px;text-align:center;transition: all 0.3s;}
.s_c_check        {position:absolute;background-image: url(<%= asset_path('s_c_check.png') %>); background-repeat: no-repeat;width:40px;height:40px;background-size:contain;display:none;}
.select_class .footer   {position: absolute; bottom: 0px;}



